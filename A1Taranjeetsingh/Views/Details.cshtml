@model StudentVM

<h2>Student Details</h2>
<table>
    <tr>
        <th>Student ID</th>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
    <tr>
        <td>@Model.Member.StudentId</td>
        <td>@Model.Member.FirstName</td>
        <td>@Model.Member.Lastname</td>
    </tr>
</table>

<h2>Courses</h2>
<table>
    <tr>
        <th>Course ID</th>
        <th>Course Name</th>
    </tr>
    @foreach (var course in Model.Courses)
    {
        <tr>
            <td>@course.CourseId</td>
            <td>@course.CourseName</td>
        </tr>
    }
</table>

}
